                            android:layout_height="wrap_content"
                            android:layout_marginTop="6dp" />

                        <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                            android:id="@+id/propertyName2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/linearLayout"
                            android:layout_marginLeft="23dp"
                            android:layout_marginStart="23dp"
                            android:layout_toEndOf="@+id/foodImage2"
                            android:layout_toRightOf="@+id/foodImage2"
                            android:layout_weight="1"
                            android:text="Event Seminar Islam Living "
                            android:textColor="#000"
                            android:textSize="20dp" />

                        <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                            android:id="@+id/street3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignLeft="@+id/propertyName2"
                            android:layout_alignStart="@+id/propertyName2"
                            android:layout_below="@+id/propertyName2"
                            android:text="14 Street/3rd Block"
                            android:textColor="#A2A2A2"
                            android:textSize="14dp" />

                        <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                            android:id="@+id/street4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignLeft="@+id/street3"
                            android:layout_alignStart="@+id/street3"
                            android:layout_below="@+id/street3"
                            android:text="Super Build-up Area:1060 Sq.Ft"
                            android:textColor="#A2A2A2"
                            android:textSize="12dp" />

                        <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                            android:id="@+id/amount2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignLeft="@+id/street4"
                            android:layout_alignStart="@+id/street4"
                            android:layout_below="@+id/street4"
                            android:text="Cp : 0895326382"
                            android:textColor="#000"
                            android:textSize="14dp" />

                    </RelativeLayout>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="5dp">

                    <RelativeLayout
                        android:id="@+id/menu3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <ImageView
                            android:id="@+id/foodImage3"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:scaleType="centerCrop"
                            android:src="@drawable/event4" />

                        <LinearLayout
