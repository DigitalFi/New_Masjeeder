                        <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                            android:id="@+id/amount9"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignLeft="@+id/street8"
                            android:layout_alignStart="@+id/street8"
                            android:layout_below="@+id/street8"
                            android:text="Cp : 0895123982379"
                            android:textColor="#000"
                            android:textSize="14dp" />

                    </RelativeLayout>
                </android.support.v7.widget.CardView>
            </LinearLayout>

        </LinearLayout>
    </LinearLayout>

</RelativeLayout>