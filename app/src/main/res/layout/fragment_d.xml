                            android:textColor="#000"
                            android:textSize="20dp" />

                        <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                            android:id="@+id/street5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignLeft="@+id/propertyName4"
                            android:layout_alignStart="@+id/propertyName4"
                            android:layout_below="@+id/propertyName4"
                            android:text="14 Street/3rd Block"
                            android:textColor="#A2A2A2"
                            android:textSize="14dp" />

                        <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                            android:id="@+id/street6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignLeft="@+id/street5"
                            android:layout_alignStart="@+id/street5"
                            android:layout_below="@+id/street5"
                            android:text="Super Build-up Area:1060 Sq.Ft"
                            android:textColor="#A2A2A2"
                            android:textSize="12dp"
                            tools:ignore="UnknownId" />

                        <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                            android:id="@+id/amount6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignLeft="@+id/street6"
                            android:layout_alignStart="@+id/street6"
                            android:layout_below="@+id/street6"
                            android:text="Cp : 0895123982379"
                            android:textColor="#000"
                            android:textSize="14dp" />

                    </RelativeLayout>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="5dp">

                    <RelativeLayout
                        android:id="@+id/menu6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <ImageView
                            android:id="@+id/foodImage"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:scaleType="centerCrop"
                            android:src="@drawable/event5" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="6dp"
                            android:id="@+id/linearLayout2"
                            tools:ignore="Orientation" />

                        <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                            android:id="@+id/propertyName7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignTop="@+id/linearLayout2"
                            android:layout_marginLeft="35dp"
                            android:layout_marginStart="35dp"
                            android:layout_toEndOf="@+id/foodImage"
                            android:layout_toRightOf="@+id/foodImage"
                            android:layout_weight="1"
                            android:text="Islamic Festival"
                            android:textColor="#000"
                            android:textSize="20dp" />

                        <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                            android:id="@+id/street7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignLeft="@+id/propertyName7"
                            android:layout_alignStart="@+id/propertyName7"
                            android:layout_below="@+id/propertyName7"
                            android:text="14 Street/3rd Block"
                            android:textColor="#A2A2A2"
                            android:textSize="14dp" />

                        <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                            android:id="@+id/street8"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignLeft="@+id/street7"
                            android:layout_alignStart="@+id/street7"
                            android:layout_below="@+id/street7"
                            android:text="Super Build-up Area:1060 Sq.Ft"
                            android:textColor="#A2A2A2"
                            android:textSize="12dp"
