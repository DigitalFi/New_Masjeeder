                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:layout_marginLeft="5dp">

                    <RelativeLayout
                        android:id="@+id/menu1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <ImageView
                            android:id="@+id/foodImage10"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:scaleType="centerCrop"
                            android:src="@drawable/event2" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="6dp" />

                        <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                            android:id="@+id/propertyName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_marginLeft="23dp"
                            android:layout_marginStart="23dp"
                            android:layout_toEndOf="@+id/foodImage10"
                            android:layout_toRightOf="@+id/foodImage10"
                            android:layout_weight="1"
                            android:text="Event Hijrah Bareng"
                            android:textColor="#000"
                            android:textSize="20dp" />

                        <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                            android:id="@+id/street1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignLeft="@+id/propertyName"
                            android:layout_alignStart="@+id/propertyName"
                            android:layout_below="@+id/propertyName"
                            android:text="14 Street/3rd Block"
                            android:textColor="#A2A2A2"
                            android:textSize="14dp" />

                        <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                            android:id="@+id/street2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignLeft="@+id/street1"
                            android:layout_alignStart="@+id/street1"
                            android:layout_below="@+id/street1"
                            android:text="Super Build-up Area:1060 Sq.Ft"
                            android:textColor="#A2A2A2"
                            android:textSize="12dp" />

                        <com.lobothijau.carouselsederhana.customfonts.MyTextView_Roboto_Regular
                            android:id="@+id/amount"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignLeft="@+id/street2"
                            android:layout_alignStart="@+id/street2"
                            android:layout_below="@+id/street2"
                            android:text="Cp : 0895345748278"
                            android:textColor="#000"
                            android:textSize="14dp" />
                    </RelativeLayout>

                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="5dp">

                    <RelativeLayout
                        android:id="@+id/menu5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <ImageView
                            android:id="@+id/foodImage2"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
